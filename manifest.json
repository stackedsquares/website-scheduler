{
    "manifest_version": 3,
    "name": "Website Scheduler",
    "version": "1.01",
    "permissions": ["alarms", "storage", "tabs"],
    "host_permissions": ["http://*/*", "https://*/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "128": "icon_128.png"
    }
}
  